    "boolean": "0.1.2",
    "bunyan": "1.8.8",
    "bunyan-format": "0.2.1",
    "bytes": "2.4.0",
    "cli-spinner": "0.2.6",
    "cli-table": "0.3.1",
    "config": "1.25.1",
    "figlet": "1.2.0",
    "fs": "0.0.2",
